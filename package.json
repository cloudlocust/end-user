{
    "name": "react-base",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "src/common"
    ],
    "dependencies": {
        "apexcharts": "^3.35.3",
        "convert-units": "^2.3.4",
        "date-fns": "^2.28.0",
        "material-ui-confirm": "^3.0.4",
        "react-apexcharts": "^1.4.0",
        "react-scripts": "4.0.3",
        "redux-persist-transform-filter": "^0.0.20",
        "typeface-poppins": "^1.1.13"
    },
    "scripts": {
        "start": "craco start",
        "build": "GENERATE_SOURCEMAP=false craco build",
        "test": "craco test --silent",
        "eject": "react-scripts eject",
        "lint-autofix": "eslint '*/**/*.{ts,tsx}' --fix",
        "help": "formatjs extract --help",
        "extract": "formatjs extract",
        "compile": "formatjs compile",
        "chelp": "formatjs compile --help",
        "ex": "node formatter.js",
        "ok1": "npm run extract -- 'src/**/*.ts*' --ignore 'src/**/*.d.ts' --out-file lang/fr.json --format simple",
        "ok": "npm run extract -- 'node_modules/react-platform-**/**/index.js*' --ignore 'src/**/*.d.ts'  --additional-function-names createElement --out-file lang/ens.json --format formatter.js",
        "mm": "formatjs compile \"node_modules/react-platform-**/**/lang/fr.json\"  --out-file lang/fr.json --format simple",
        "prepare": "husky install",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "pre-commit": "lint-staged",
        "serveBuild": "yarn build && npx serve -s build"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/runtime": "^7.16.7",
        "@craco/craco": "^6.4.1",
        "@formatjs/cli": "^3.1.0",
        "@storybook/addon-actions": "^6.3.12",
        "@storybook/addon-essentials": "^6.4.9",
        "@storybook/addon-links": "^6.3.12",
        "@storybook/cli": "^6.3.12",
        "@storybook/node-logger": "^6.3.12",
        "@storybook/preset-create-react-app": "^3.2.0",
        "@storybook/react": "^6.3.12",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^11.1.0",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^12.1.10",
        "@types/autosuggest-highlight": "^3.2.0",
        "@types/chance": "^1.1.3",
        "@types/classnames": "^2.2.11",
        "@types/convert-units": "^2.3.5",
        "@types/jest": "^26.0.15",
        "@types/lodash": "^4.14.167",
        "@types/node": "^17.0.6",
        "@types/qs": "^6.9.5",
        "@types/react": "^17.0.0",
        "@types/react-dates": "^21.8.3",
        "@types/react-dom": "^17.0.0",
        "@types/react-redux": "^7.1.21",
        "@types/react-router-config": "^5.0.2",
        "@types/react-router-dom": "^5.1.6",
        "autoprefixer": "^9",
        "babel-loader": "8.1.0",
        "chance": "^1.1.8",
        "copyfiles": "^2.4.1",
        "dotenv": "^16.0.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-css-modules": "^2.11.0",
        "eslint-plugin-jsdoc": "^37.0.3",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-sonarjs": "^0.11.0",
        "factory.ts": "^0.5.2",
        "husky": "^6.0.0",
        "jest-sonar-reporter": "^2.0.0",
        "jsdoc": "^3.6.7",
        "lint-staged": "^11.0.0",
        "msw": "^0.29.0",
        "node-sass": "6.0.0",
        "postcss": "^8.2.4",
        "prettier": "^2.2.1",
        "prop-types": "^15.8.0",
        "sb": "^6.3.12",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat",
        "tailwindcss-dir": "4.0.0",
        "ts-jest": "26.5.6",
        "type-fest": "^2.11.2",
        "typescript": "^4.3.2",
        "web-vitals": "^1.0.1"
    },
    "jestSonar": {
        "reportPath": "reports",
        "reportFile": "test-report.xml",
        "indent": 4
    }
}
